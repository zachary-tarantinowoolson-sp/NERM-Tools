{
    "name": "SecZetta Swap",
    "version": "1.0.1",
    "description": "Swaps Profile pages between Admin and Dashboard",
    "manifest_version": 3,
    "author": "Zachary Tarantino",
    "icons": {
       "16": "icons/icon_16.png",
       "32": "icons/icon_32.png",
       "48": "icons/icon_48.png",
       "128": "icons/icon_128.png"
    },
    "action": {
      "default_popup": "background.html",
      "default_title": "Swap Pages"
    },
    "options_page":"options.html",
    "permissions": [
        "tabs",
        "storage",
        "activeTab"
    ],
    "background": {
      "service_worker": "service-worker.js"
    },
    "content_scripts":[{
      "js":["swap.js"],
      "matches": ["http://*/*", "https://*/*"],
      "run_at": "document_end"
    }]
}